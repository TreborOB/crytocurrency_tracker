{
  "name": "crytocurrency_tracker",
  "version": "1.0.0",
  "description": "crytocurrency_tracker",
  "main": "./src/index.js",
  "scripts": {
    "build-babel": "babel -d ./build ./src -s",
    "start": "nodemon --ignore cryto_tracker/* --exec babel-node src/index.js",
    "test": "cross-env NODE_ENV=test mocha ./src/test --require babel-core/register --require babel-polyfill --reporter mochawesome --exit",
    "clean": "rimraf build && mkdir build",
    "build": "npm run clean && npm run build-babel",
    "start-prod": "npm run build",
    "copy-client": "cpx \"client/build/**/*\"  build/public",
    "start-dev": "nodemon --exec babel-node src/index.js",
    "build-prod-client": "npm run clean-client && npm run build-client && npm run copy-client",
    "build-client": "cd client && npm run build",
    "clean-client": "rimraf client/build && rimraf ./build/public",
    "install-client-mods": "cd client/ && npm install && npm install --only=dev --no-shrinkwrap",
    "heroku-postbuild": "npm run build && npm run install-client-mods && npm run build-prod-client"
  },
  "author": "robrien",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cpx": "^1.5.0",
    "eslint": "^4.19.1",
    "eslint-config-google": "^0.9.1",
    "mocha": "^5.1.1",
    "mochawesome": "^3.0.2",
    "mockgoose": "^7.3.5",
    "nodemon": "^1.17.4",
    "rimraf": "^2.6.2",
    "should": "^13.2.1",
    "supertest": "^3.1.0"
  },
  "dependencies": {
    "@ljharb/eslint-config": "^12.2.1",
    "axios": "^0.18.0",
    "bcrypt": "^2.0.1",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.3",
    "cross-env": "^5.1.5",
    "dotenv": "^4.0.0",
    "eslint-config-tinchoz49": "^2.1.0",
    "eslint-plugin-react": "^7.8.2",
    "express": "^4.16.3",
    "express-async-handler": "^1.1.3",
    "jsonwebtoken": "^8.2.1",
    "lodash": "^4.17.10",
    "mongodb": "^3.1.0-beta4",
    "mongoose": "^5.1.1",
    "morgan": "^1.9.0",
    "npm": "^6.0.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "regenerator-runtime": "^0.11.1",
    "save-dev": "^2.0.0",
    "serve-favicon": "^2.5.0"
  }
}
